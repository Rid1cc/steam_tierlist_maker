# Environment variables for development
# Copy this file to .env.local and fill in your Steam API key

# Steam Web API Key - get it from: https://steamcommunity.com/dev/apikey
STEAM_API_KEY=your_steam_api_key_here

# Steam User ID (76561198xxxxxxxxx format) - optional for demo
STEAM_USER_ID=your_steam_id_here

# Next.js environment
NEXT_PUBLIC_APP_URL=http://localhost:3000